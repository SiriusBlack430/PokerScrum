{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "semi": [2,"never"],
        "no-plusplus":0,
        "no-underscore-dangle":0,
        "no-console":"off",
        "consistent-return":"off"
    }
}
// npm init @eslint/config
